app.config(function($routeProvider){
    $routeProvider.when("/sentiero",{
        templateUrl:"ragazzo/sentiero/sentiero.html"
    }).when("/profilo",{
        templateUrl:"ragazzo/profilo/profilo.html"
    }).when("/fiamma",{
        templateUrl:"capo/fiamma/fiamma.html"
    }).otherwise({
        redirectTo:"index.html"
    })
});